    <fields>
        <fullName>Contract_Months__c</fullName>
        <description>Calculates the exact number of months the contract is for based on Contract Start Date and Contract End Date.</description>
        <externalId>false</externalId>
        <formula>((YEAR( Contract_End_Date__c+1) - YEAR( Contract_Start_Date__c)) *12) 
+ (MONTH( Contract_End_Date__c+1) - MONTH( Contract_Start_Date__c)) 
+ (Day(Contract_End_Date__c+1) - Day(Contract_Start_Date__c)) 
/( 
if(month(Contract_End_Date__c)=12,31, 
