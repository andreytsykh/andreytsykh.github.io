),
IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_260))+&quot;img/msg_icons/error16.png&quot;,&quot;&gt; 10 Days&quot;),
IF(
Days_Since_Next_Steps_Updated__c &gt;7,
IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_260))+&quot;img/msg_icons/warning16.png&quot;,&quot;&gt; 7 Days&quot;),
IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_260))+&quot;img/msg_icons/confirm16.png&quot;,&quot;Up to Date&quot;)
)))</formula>
