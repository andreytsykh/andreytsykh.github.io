    <fieldSets>
        <fullName>test</fullName>
        <description>fdsdfs</description>
        <label>test</label>
    </fieldSets>
    <fields>
        <fullName>ARR_Weighted__c</fullName>
        <description>ARR weighted based on stage probability.</description>
        <externalId>false</externalId>
        <formula>ARR__c*Probability</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ARR Weighted</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ARR__c</fullName>
        <description>Recurring Amount divided by the number of Contract Months in the term, multiplied by 12 to get the Annual Value.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(MRR__c), Amount, MRR__c*12)</formula>
        <inlineHelpText>Annual Recurring Revenue</inlineHelpText>
        <label>ARR</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>AccountId</fullName>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Amount</fullName>
        <inlineHelpText>Total Contract Value (Default Amount Field)</inlineHelpText>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
    </fields>
    <fields>
        <fullName>Amount_per_Payment__c</fullName>
        <description>Divides the TCV by the Number of Payments for finance to know how much to invoice each time.</description>
        <externalId>false</externalId>
        <formula>Amount/Number_of_Payments__c</formula>
        <inlineHelpText>Calculates the payment amount per invoice based on TCV and Number of Payments.</inlineHelpText>
        <label>Amount per Payment</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Annual_Discount_from_Rate_Card__c</fullName>
        <description>Returns the % discount from suggested rate card (via pricing matrix formula) only if there is a discount. Otherwise it returns 0%.</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
Annual_Fee_Quote__c &gt; Recurring_Total__c,
Recurring_Total__c &gt; 0
),

(Annual_Fee_Quote__c - Recurring_Total__c)/Annual_Fee_Quote__c,
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The % discount from what the Pricing Matrix is saying against what you currently value the Opportunity at.</inlineHelpText>
        <label>Annual Discount from Rate Card</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Annual_Fee_Quote__c</fullName>
        <description>References the Pricing Matrix Annual Fee Number and returns the associated quote amount for that value.</description>
        <externalId>false</externalId>
        <formula>IF(Pricing_Matrix_Annual_Fee_Number__c &lt; 10, 7500,

CASE(Pricing_Matrix_Annual_Fee_Number__c, 
10, 8000, 
10.5, 8000, 
11, 8000, 
11.5, 9000, 
12, 9000, 
12.5, 10000, 
13, 13000, 
13.5, 13000, 
14, 14000, 
14.5, 15000, 
15, 15000, 
15.5, 15000, 
16, 16000, 
16.5, 18000, 
17, 20000, 
17.5, 20000, 
18, 22000, 
18.5, 25000, 
19, 28000, 
19.5, 30000, 
20, 30000, 
20.5, 33000, 
21, 35000, 
21.5, 35000,
22, 38000, 
22.5, 38000, 
23, 38000, 
23.5, 38000, 
24, 38000, 
24.5, 40000, 
25, 40000, 
25.5, 43000, 
26, 45000, 
26.5, 45000, 
50000)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The suggested annual fee for this customer per the Pricing Matrix.</inlineHelpText>
        <label>Annual Fee Quote</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>At_Risk_Date__c</fullName>
        <description>The date the Renewal was marked At Risk.</description>
        <externalId>false</externalId>
        <inlineHelpText>When the Renewal was marked At Risk.</inlineHelpText>
        <label>At Risk Date</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>At_Risk_Flag__c</fullName>
        <description>Visual cue when looking at Renewal to quickly see if it&apos;s been marked &quot;At Risk&quot;.</description>
        <externalId>false</externalId>
        <formula>IF(
At_Risk__c = FALSE,
IMAGE(&quot;/img/msg_icons/confirm24.png&quot;, &quot;Not At Risk&quot;),
IMAGE(&quot;/img/msg_icons/error24.png&quot;, &quot;At Risk&quot;)
