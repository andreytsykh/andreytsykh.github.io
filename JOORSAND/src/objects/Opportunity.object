    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>Amount_Always_Required</fullName>
        <active>true</active>
        <description>All Opportunities must have an Amount (TCV is the custom label). Only exception is the first New Business stage &quot;First Meeting Scheduled&quot; which is before the first conversation has taken place, Closed Lost, and Partnership Opportunities.</description>
        <errorConditionFormula>AND(
AND(
NOT(ISPICKVAL(StageName, &quot;00_Expected Churn&quot;)),
NOT(ISPICKVAL(StageName, &quot;00_Nurturing&quot;)),
NOT(ISPICKVAL(StageName, &quot;01_Qualified&quot;)),
NOT(ISPICKVAL(StageName, &quot;02_Prospect Outreach&quot;)),
NOT(ISPICKVAL(StageName, &quot;03_First Meeting Scheduled&quot;)),
NOT(ISPICKVAL(StageName, &quot;04_Discovery&quot;)),
NOT(ISPICKVAL(StageName, &quot;12_Closed Lost&quot;)),
NOT(ISPICKVAL(Type, &quot;Partnership&quot;))
),

OR(
ISBLANK(Amount)
)
)</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Amount (TCV) is required on all Opportunities. Please estimate the value of the Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>At_Risk_Renewals_Require_Notes</fullName>
        <active>true</active>
        <description>Ensures any renewal marked at risk has notes to explain the situation.</description>
        <errorConditionFormula>AND(
At_Risk__c = TRUE,
ISBLANK(At_Risk_Notes__c)
)</errorConditionFormula>
        <errorDisplayField>At_Risk_Notes__c</errorDisplayField>
        <errorMessage>Notes are required for At Risk Renewals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Cannot_be_in_the_Past</fullName>
        <active>true</active>
        <description>Cannot edit the Opportunity if it&apos;s open and the Close Date is in the past.</description>
        <errorConditionFormula>AND(
NOT(IsClosed),
CloseDate &lt; TODAY()
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Close Date cannot be in the past on open Opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Months_Must_be_Even</fullName>
        <active>true</active>
        <description>Ensures no partial month contract dates are entered into Opportunities when they&apos;re won. NOTE: It&apos;s only when they&apos;re won because renewals could be created with contract months uneven because of leap year and will be fixed after.</description>
        <errorConditionFormula>AND(
IsWon = TRUE,
FLOOR(Contract_Months__c) != Contract_Months__c
)</errorConditionFormula>
        <errorDisplayField>Contract_End_Date__c</errorDisplayField>
        <errorMessage>Contract months must be a whole number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DM_and_Champion_Required</fullName>
        <active>true</active>
        <description>No opportunity can can move past early stages without a Decision Maker and Champion documented.</description>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(StageName, &quot;00_Nurturing&quot;)),
NOT(ISPICKVAL(StageName, &quot;01_Qualified&quot;)),
NOT(ISPICKVAL(StageName, &quot;02_Prospect Outreach&quot;)),
NOT(ISPICKVAL(StageName, &quot;03_First Meeting Scheduled&quot;)),
NOT(ISPICKVAL(StageName, &quot;05_Demo Completed&quot;)),
NOT(ISPICKVAL(StageName, &quot;04_Discovery&quot;)),
NOT(ISPICKVAL(StageName, &quot;05_Open Renewal&quot;)),
NOT(ISPICKVAL(StageName, &quot;00_Expected Churn&quot;)),
NOT(ISPICKVAL(StageName, &quot;12_Closed Lost&quot;)),

NOT(ISPICKVAL(Type, &quot;Partnership&quot;)),

OR(
ISBLANK(Decision_Maker__c),
ISBLANK(Champion__c)
)
)</errorConditionFormula>
        <errorDisplayField>Decision_Maker__c</errorDisplayField>
        <errorMessage>Decision Maker and Champion must be documented to move into or passed Decision Maker Engaged stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_Churn_Renewals_Must_be_At_Risk</fullName>
        <active>true</active>
        <description>Whenever a Renewal is Expected Churn, it cannot be unmarked &quot;At Risk&quot;.</description>
        <errorConditionFormula>AND(
ISPICKVAL(StageName, &quot;00_Expected Churn&quot;),
ISCHANGED(At_Risk__c),
At_Risk__c = FALSE
)</errorConditionFormula>
        <errorDisplayField>At_Risk__c</errorDisplayField>
        <errorMessage>Expected Churn Renewals cannot be unmarked &quot;At Risk&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Future_Interest_Requires_a_Date</fullName>
        <active>true</active>
        <description>When Future Interest is set to &quot;Yes&quot;, a Future Interest Date is also required and cannot be in the past.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Future_Interest__c, &quot;Yes&quot;),

OR(
ISBLANK(Future_Interest_Date__c),
Future_Interest_Date__c &lt; TODAY()
)
)</errorConditionFormula>
        <errorDisplayField>Future_Interest_Date__c</errorDisplayField>
        <errorMessage>Future Interest requires a Future Interest Date (cannot be in the past).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>JOOR_ID_Required</fullName>
        <active>true</active>
        <description>Require a JOOR ID in order to win an opportunity.</description>
        <errorConditionFormula>AND( 
IsWon = TRUE, 
ISBLANK(Account.JOOR_ID__c)
)</errorConditionFormula>
        <errorMessage>JOOR ID is missing on the Account. Please go to the account details page and input the JOOR ID.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Opportunities_Require_Loss_Reason</fullName>
        <active>true</active>
        <description>Loss Reason must be filled in when Opportunity is Closed Lost.</description>
        <errorConditionFormula>AND(
ISPICKVAL(StageName, &quot;12_Closed Lost&quot;),

OR(
ISBLANK(TEXT(Loss_Reason__c)),
ISBLANK(TEXT(Future_Interest__c)),
ISBLANK(Loss_Reason_Details__c)
)
)</errorConditionFormula>
        <errorDisplayField>Loss_Reason__c</errorDisplayField>
        <errorMessage>Closed Lost Opportunities require a Loss Reason w/ Details and a value for Future Interest.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Renewal_ARR_Must_Match_Source</fullName>
        <active>true</active>
        <description>If churning a renewal, the ARR must match the Source Opportunity ARR to ensure accurate churn/retention reporting on ARR.</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Source_Opportunity__c)),
Source_Opportunity_ARR__c != ARR__c,
ISPICKVAL(StageName, &quot;12_Closed Lost&quot;)
)</errorConditionFormula>
        <errorDisplayField>Source_Opportunity_ARR__c</errorDisplayField>
        <errorMessage>To churn this contract, the ARR must match the Source Opportunity ARR. You will likely need to adjust either the Product values or Contract Dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_System_Admins_Can_Edit_Closed_Opps</fullName>
        <active>true</active>
        <description>Prevents everyone but System Administrators from editing key fields on closed Opportunities.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,

OR(
  
  AND(
   IsClosed = TRUE,
   PRIORVALUE(IsClosed) = TRUE, 
   OR(
   ISCHANGED(Contract_Start_Date__c),
   ISCHANGED(Contract_End_Date__c),
   ISCHANGED(Amount),
   ISCHANGED(CloseDate),
   ISCHANGED(StageName),
   ISCHANGED(Probability),
   ISCHANGED(ForecastCategoryName),
   ISCHANGED(Billing_Details__c),
   ISCHANGED(Billing_Term__c),
   ISCHANGED(Number_of_Payments__c),
   ISCHANGED(Auto_Renew__c)
   )
  ),
 
 AND(
 IsClosed = FALSE,
 PRIORVALUE(IsClosed) = TRUE
 )

)

)</errorConditionFormula>
        <errorMessage>Only System Administrators can edit financial fields or re-open closed Opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnership_Billing_When_Revenue_Present</fullName>
        <active>true</active>
        <description>Enforces billing field validation and product entry whenever revenue is present.</description>
        <errorConditionFormula>AND( 
IsWon, 
ISPICKVAL(Type, &quot;Partnership&quot;),
Amount &gt; 0,

OR( 
ISBLANK(TEXT(Number_of_Payments__c)), 
ISBLANK(TEXT(Billing_Term__c)), 
ISBLANK(Billing_Contact__c), 
ISBLANK(TEXT(Auto_Renew__c)), 
ISBLANK(Contract_Start_Date__c), 
ISBLANK(Contract_End_Date__c),
ISBLANK(Renewal_Price_Increase__c),
of_Product_Line_Items__c = 0 
) 
)</errorConditionFormula>
        <errorMessage>Revenue generating Partnerships require all billing details, contract dates, auto-renew, renewal price increase, and product(s) to be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnership_Dev_Stage_Requirements</fullName>
        <active>true</active>
        <description>Required fields to enter Development Stage in Partnership Opportunities.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;Partnership&quot;), 
ISPICKVAL(StageName, &quot;Development&quot;), 

OR( 
ISBLANK(TEXT(Development_Required_By__c)), 
ISBLANK(Dev_Target_Due_Date__c)
)
)</errorConditionFormula>
        <errorDisplayField>Development_Required_By__c</errorDisplayField>
        <errorMessage>Development Required By and Dev Target Due Date are required to move the stage to Development.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnership_Discovery_Exit_Requirements</fullName>
        <active>true</active>
        <description>Requires various pieces of information in order to move past the Partnership Opportunity Stage &quot;Discovery&quot;.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Type, &quot;Partnership&quot;),
NOT(ISPICKVAL(StageName, &quot;Discovery&quot;)),
NOT(ISPICKVAL(StageName, &quot;Outreach&quot;)),
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)),

OR(
ISBLANK(Partner__c),
ISBLANK(TEXT(Integration_Type__c)),
ISBLANK(Integration__c)
)
)</errorConditionFormula>
        <errorDisplayField>Partner__c</errorDisplayField>
        <errorMessage>Partner, Integration Type, and Integration are required to move past the Discovery stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnership_Pilot_Exit_Stage_Requirement</fullName>
        <active>true</active>
        <description>Documentation must be Completed in order to leave the Piloting stage.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;Partnership&quot;), 
TEXT(PRIORVALUE(StageName)) = &quot;Piloting&quot;, 
NOT(ISPICKVAL(StageName, &quot;Piloting&quot;)),
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)),
NOT(ISPICKVAL(Documentation__c, &quot;Complete&quot;))
)</errorConditionFormula>
        <errorDisplayField>Documentation__c</errorDisplayField>
        <errorMessage>Documentation must be &quot;Complete&quot; in order to leave the Piloting stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnership_Piloting_Stage_Requirements</fullName>
        <active>true</active>
        <description>Fields required for Partnership Opportunities to enter the Piloting stage.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;Partnership&quot;), 
ISPICKVAL(StageName, &quot;Piloting&quot;), 

OR( 
ISBLANK(Pilot_Account__c), 
ISBLANK(Pilot_Target_Due_Date__c)
)
)</errorConditionFormula>
        <errorDisplayField>Pilot_Account__c</errorDisplayField>
        <errorMessage>Pilot Account and Pilot Target Due Date are required to change stage to Piloting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnership_Testing_Stage_Requirements</fullName>
        <active>true</active>
        <description>Required fields for Partnership Opportunities to move into the Testing stage.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;Partnership&quot;), 
ISPICKVAL(StageName, &quot;Testing&quot;), 

OR( 
ISBLANK(JOOR_Test_Account__c), 
ISBLANK(Testing_Target_Due_Date__c)
)
)</errorConditionFormula>
        <errorDisplayField>JOOR_Test_Account__c</errorDisplayField>
        <errorMessage>JOOR Test Account and Testing Target Due Date are required to move the Stage to Testing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pricing_Matrix_Required_for_Brand_Deals</fullName>
        <active>true</active>
        <description>Requires all the Pricing Matrix data to be entered before moving stage to Pricing Negotiations+ is the Account&apos;s Business Category is &quot;Brand&quot;.</description>
        <errorConditionFormula>AND( 
NOT(ISPICKVAL(StageName, &quot;00_Nurturing&quot;)),
NOT(ISPICKVAL(StageName, &quot;01_Qualified&quot;)),
NOT(ISPICKVAL(StageName, &quot;02_Prospect Outreach&quot;)),
NOT(ISPICKVAL(StageName, &quot;03_First Meeting Scheduled&quot;)), 
NOT(ISPICKVAL(StageName, &quot;05_Demo Completed&quot;)), 
NOT(ISPICKVAL(StageName, &quot;04_Discovery&quot;)), 
NOT(ISPICKVAL(StageName, &quot;05_Open Renewal&quot;)), 
NOT(ISPICKVAL(StageName, &quot;06_Decision Maker Engaged&quot;)), 
NOT(ISPICKVAL(StageName, &quot;07_Verbal&quot;)), 
NOT(ISPICKVAL(StageName, &quot;00_Expected Churn&quot;)),
NOT(ISPICKVAL(StageName, &quot;12_Closed Lost&quot;)), 

ISPICKVAL(Account.Business_Category__c, &quot;Brand&quot;),

OR( 
ISBLANK(TEXT(of_SEASONS_per_year__c)),
ISBLANK(TEXT(of_USERS__c)),
ISBLANK(TEXT(of_STOCKISTS__c)),
of_Product_Line_Items__c = 0,
ISBLANK(TEXT(Wholesale_VOLUME__c))
) 
)</errorConditionFormula>
        <errorMessage>Pricing Matrix fields must all be entered and Products should be added to the Opportunity to move Stage to Pricing Negotiations+</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pricing_Matrix_Required_for_Demo_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;New Business&quot;), 
NOT(ISPICKVAL(StageName, &quot;00_Nurturing&quot;)), 
NOT(ISPICKVAL(StageName, &quot;01_Qualified&quot;)), 
NOT(ISPICKVAL(StageName, &quot;02_Prospect Outreach&quot;)), 
NOT(ISPICKVAL(StageName, &quot;03_First Meeting Scheduled&quot;)),  
NOT(ISPICKVAL(StageName, &quot;04_Discovery&quot;)),
NOT(ISPICKVAL(StageName, &quot;12_Closed Lost&quot;)), 

OR( 
ISBLANK(TEXT(of_SEASONS_per_year__c)), 
ISBLANK(TEXT(of_USERS__c)), 
ISBLANK(TEXT(of_STOCKISTS__c)), 
ISBLANK(TEXT(Wholesale_VOLUME__c)) 
)
)</errorConditionFormula>
        <errorMessage>Requires all the Pricing Matrix data to be entered before moving stage to Demo Completed+</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Billing_Fields_to_Close_Win</fullName>
        <active>true</active>
        <description>Forces entry of all Billing/Finance related fields in order to close win the Opportunity, with the exception being Partnership Opportunities which do not include revenue.</description>
        <errorConditionFormula>AND(
IsWon,
NOT(ISPICKVAL(Type, &quot;Partnership&quot;)),

OR(
ISBLANK(TEXT(Number_of_Payments__c)),
ISBLANK(TEXT(Billing_Term__c)),
ISBLANK(Billing_Contact__c),
ISBLANK(TEXT(Auto_Renew__c)),
ISBLANK(Contract_Start_Date__c),
ISBLANK(Renewal_Price_Increase__c),
ISBLANK(TEXT(Pilot__c)),
ISBLANK(Contract_End_Date__c)
)
)</errorConditionFormula>
        <errorMessage>Billing Details, Contract Dates, Pilot, Auto Renew, and Renewal Increase % are required to mark the Opportunity as Closed Won. Tip: Mark values as zero, &apos;No&apos;, etc. as blank fields are not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Brands_Retailers_to_Close_Win</fullName>
        <active>true</active>
        <description>Forces entry of at least one Brand/Retailer in order to close win the Opportunity, only exception is for Partnership Opportunities which do not involve products or revenue.</description>
        <errorConditionFormula>AND(
IsWon,
NOT(ISPICKVAL(Type, &quot;Partnership&quot;)),
of_Brands_Retailers_Attached__c = 0
)</errorConditionFormula>
        <errorMessage>At least one Brand/Retailer must be attached in order to mark the Opportunity as Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_First_Meeting_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;New Business&quot;), 
NOT(ISPICKVAL(StageName, &quot;00_Nurturing&quot;)), 
NOT(ISPICKVAL(StageName, &quot;01_Qualified&quot;)), 
NOT(ISPICKVAL(StageName, &quot;02_Prospect Outreach&quot;)),  

ISBLANK(First_Meeting_Date__c)
)</errorConditionFormula>
        <errorDisplayField>First_Meeting_Date__c</errorDisplayField>
        <errorMessage>First Meeting Date is required to move Stage to First Meeting Scheduled+</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Must_Be_Before_End_Date</fullName>
        <active>true</active>
        <description>Ensures the Contract Start Date is always before the Contract End Date.</description>
        <errorConditionFormula>Contract_End_Date__c &lt; Contract_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Contract_Start_Date__c</errorDisplayField>
        <errorMessage>Contract Start Date must be before Contract End Date.</errorMessage>
    </validationRules>
