    <fields>
        <fullName>Case_Age__c</fullName>
        <description>Calculates the days the case has been open, or how long it was open before becoming closed.</description>
        <externalId>false</externalId>
        <formula>IF(IsClosed,
DATEVALUE(ClosedDate)-DATEVALUE(CreatedDate),
TODAY()-DATEVALUE(CreatedDate)
)</formula>
        <label>Case Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
