    <validationRules>
        <fullName>Case_as_Status_minutes</fullName>
        <active>true</active>
        <description>Minutes Spent and Resolution should only be required to set Case as Status &gt; Closed or Resolved</description>
        <errorConditionFormula>AND(
        ISBLANK( Minutes_Spent__c ),   
        OR(
           ISPICKVAL (Status , &quot;Closed&quot;),
           ISPICKVAL (Status , &quot;Resolved&quot;)
        )        
)</errorConditionFormula>
        <errorMessage>To close the case, fill Minutes Spent field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_as_Status_resolution</fullName>
        <active>true</active>
        <description>Minutes Spent and Resolution should only be required to set Case as Status &gt; Closed or Resolved</description>
        <errorConditionFormula>AND(
        ISBLANK( Resolution__c ),
        OR(
           ISPICKVAL (Status , &quot;Closed&quot;),
           ISPICKVAL (Status , &quot;Resolved&quot;)
        )        
)</errorConditionFormula>
        <errorMessage>To close the case, fill Resolution field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Case_Required_Fields</fullName>
        <active>false</active>
        <description>Forces certain fields to be filled in when closing a case. Requires validation rule because Closed Case page layout does not apply to lightning edition, so we can&apos;t require those fields during the close case process without a validation rule.</description>
        <errorConditionFormula>AND(
IsClosed,

OR(
ISBLANK(Resolution__c),
ISBLANK(Minutes_Spent__c)
)
)</errorConditionFormula>
        <errorMessage>Resolution and Minutes Spent are required to close this Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Case_Required_Fields_New</fullName>
        <active>true</active>
        <description>Forces certain fields to be filled in when closing a case. Requires validation rule because Closed Case page layout does not apply to lightning edition, so we can&apos;t require those fields during the close case process without a validation rule.</description>
        <errorConditionFormula>AND(
 PRIORVALUE( IsClosed ) = False,
 IsClosed,

OR(
ISBLANK(Resolution__c),
ISBLANK(Minutes_Spent__c)
)
)</errorConditionFormula>
        <errorMessage>Resolution and Minutes Spent are required to close this Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Minutes_Spent</fullName>
        <active>true</active>
        <description>Prohibits negative value of spent minutes</description>
        <errorConditionFormula>Minutes_Spent__c &lt; 0</errorConditionFormula>
        <errorMessage>The time spent can not be a negative value</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Quick_Close</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Quick Close</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/33.0/connection.js&apos;)}
 
if(&apos;{!Case.IsClosed}&apos; == true){
	alert(&apos;This case is already closed.&apos;);
}else{
	var c = new sforce.SObject(&apos;Case&apos;);
	c.Id = &apos;{!Case.Id}&apos;;
	c.Status = &apos;Closed&apos;;
	var result = sforce.connection.update([c]);
 
	if (result[0].success == &apos;false&apos;) {
		alert(result[0].errors.message);
	} else {
		var cc = new sforce.SObject(&apos;CaseComment&apos;);
		cc.ParentId = &apos;{!Case.Id}&apos;;
		cc.CommentBody = &apos;Closed by portal user&apos;;
		sforce.connection.emailHeader = {triggerUserEmail:true};
		var result = sforce.connection.create([cc]);
 
		if (result[0].success == &apos;false&apos;) {
			alert(result[0].errors.message);
		} else {
			location.reload(true);
		}
	}	 
}</url>
    </webLinks>
