    <description>Automatically created renewals whenever an Opportunity with a recurring value is Closed Won.</description>
    <formulas>
        <name>RenewalContractEndDate</name>
        <dataType>Date</dataType>
        <expression>{!RenewalContractStartDate}+{!RenewalContractLength}</expression>
    </formulas>
    <formulas>
        <name>RenewalContractLength</name>
        <dataType>Number</dataType>
        <expression>{!SourceContractEndDate}-{!SourceContractStartDate}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>RenewalContractStartDate</name>
        <dataType>Date</dataType>
        <expression>{!SourceContractEndDate}+1</expression>
    </formulas>
    <interviewLabel>Auto Create Renewal {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Auto Create Renewal</label>
    <loops>
        <name>Transfer_Brands_Retailers</name>
        <label>Transfer Brands/Retailers</label>
        <locationX>773</locationX>
        <locationY>415</locationY>
        <assignNextValueToReference>BrandRetailer</assignNextValueToReference>
        <collectionReference>SourceBrandsRetailers</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Assign_Source_Brand_Retailer_to_Renewal</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>CreateRenewalProducts</targetReference>
        </noMoreValuesConnector>
    </loops>
    <loops>
        <name>TransferSourceProducts</name>
        <label>TransferSourceProducts</label>
        <locationX>235</locationX>
        <locationY>421</locationY>
