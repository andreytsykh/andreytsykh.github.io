        <collectionReference>SourceProductLineItems</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Assign_Source_Product_to_Renewal</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Brands_Retailers</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processType>AutoLaunchedFlow</processType>
